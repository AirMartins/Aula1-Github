package principal;

import java.util.Scanner;

public class exer01 {
	

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
	
         System.out.println("digite um numero: ");
		int num = sc.nextInt();

		if (num< 0) {
			System.out.println("Número Negativo");
		} else {
			System.out.println("Não Negativo: ");

			sc.close();

		}
	}

}
