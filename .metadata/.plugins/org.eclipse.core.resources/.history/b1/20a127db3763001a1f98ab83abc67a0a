//estrutura repetitiva For: 02
package Principal;

import java.util.Scanner;

public class exer02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		System.out.println("digite um numero");
		int num = sc.nextInt();
		int in=0;
		int ou=0;		

		for (int i = 0; i<num; i++) {
			System.out.println("digite um numero");
			int x = sc.nextInt();
			if (x >= 10 && x <= 20) {
				in = in + 1;
			}
			else {
				ou = ou + 1;
			}
		

		sc.close();
	}
	}

}
