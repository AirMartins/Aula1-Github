
public class Main {

	public static void main(String[] args) {
		System.out.print("Olá Mundo! ");
		System.out.println("Bom Dia ");
		int y=32;
		double x = 10.35784;
		System.out.println("a variável Y contém " + y  + " e variável x " + x);
		System.out.printf("%.2f%n", x);// %n tem o mesmo efeito de \n de qubra de linha
		System.out.printf("%.4f%n", x);
	}

}
